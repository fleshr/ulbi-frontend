{
  "name": "ulbi-frontend",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:dev": "concurrently \"npm run start\" \"npm run start:dev:server\"",
    "start:dev:server": "ts-node ./json-server/server.ts",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "lint:ts": "eslint **/*.{ts,tsx}",
    "lint:ts:fix": "eslint **/*.{ts,tsx} --fix",
    "lint:scss": "stylelint **/*.scss",
    "lint:scss:fix": "stylelint **/*.scss --fix",
    "typecheck": "tsc --noEmit",
    "test:unit": "jest --runInBand --config ./config/jest/jest.config.ts",
    "test:ui": "testplane --storybook --config ./config/testplane/testplane.storybook.config.ts",
    "test:ui:update": "testplane --storybook --update-refs --config ./config/testplane/testplane.storybook.config.ts",
    "test:ui:gui": "testplane gui --storybook --config ./config/testplane/testplane.storybook.config.ts",
    "storybook": "storybook dev --port 6006 --config-dir ./config/storybook",
    "storybook:build": "storybook build --config-dir ./config/storybook",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.0.2",
    "@eslint/js": "^9.24.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.16",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-webpack5": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@testplane/storybook": "^1.6.1",
    "@types/jest": "^29.5.14",
    "@types/json-server": "^0.14.8",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-i18next": "^6.1.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.12.0",
    "file-loader": "^6.2.0",
    "globals": "^16.0.0",
    "html-reporter": "^10.16.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jiti": "^2.4.2",
    "json-server": "^0.17.4",
    "lint-staged": "^15.5.1",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.5.3",
    "react-refresh-typescript": "^2.0.10",
    "sass": "^1.86.3",
    "sass-loader": "^16.0.5",
    "storybook": "^8.6.12",
    "style-loader": "^4.0.0",
    "stylelint": "^16.18.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-prettier": "^5.0.3",
    "testplane": "^8.28.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "webpack": "^5.99.5",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.7.0",
    "@tanstack/react-virtual": "^3.13.6",
    "axios": "^1.8.4",
    "i18next": "^25.0.0",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.4.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.0"
  }
}
